{
	"name": "natural-highs",
	"type": "module",
	"license": "MIT",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "doppler run -c dev -- vite",
		"dev:bare": "vite",
		"build": "vite build",
		"build:stg": "doppler run -c stg -- vite build",
		"build:prd": "doppler run -c prd -- vite build",
		"start": "bun .output/server/index.mjs",
		"emulators": "firebase emulators:start",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:unit": "vitest src/lib src/server",
		"test:component": "vitest src/components",
		"test:coverage": "vitest run --coverage && npx open-cli coverage/index.html",
		"test:create": "bun scripts/create-test.ts",
		"test:e2e": "playwright test --ui",
		"test:ci": "vitest run",
		"test:e2e:ci": "playwright test",
		"lint:tsc": "tsc -b",
		"lint:biome": "biome check --write --unsafe",
		"lint": "bun run lint:tsc && bun run lint:biome",
		"format": "biome format --write",
		"validate": "bun run lint && bun run test:ci && bun run test:e2e:ci",
		"ci:local": "bash scripts/ci-local.sh",
		"test:burn-in": "bash scripts/burn-in.sh",
		"setup": "doppler login && doppler setup",
		"trunk": "trunk",
		"trunk:check": "trunk check",
		"trunk:fmt": "trunk fmt",
		"prepare": "husky"
	},
	"dependencies": {
		"@radix-ui/react-checkbox": "1.3.3",
		"@radix-ui/react-dialog": "1.1.15",
		"@radix-ui/react-label": "2.1.8",
		"@radix-ui/react-select": "2.2.6",
		"@radix-ui/react-separator": "1.1.8",
		"@radix-ui/react-slot": "1.2.4",
		"@radix-ui/react-tabs": "1.1.13",
		"@tanstack/match-sorter-utils": "8.19.4",
		"@tanstack/react-form": "1.27.5",
		"@tanstack/react-query": "5.90.12",
		"@tanstack/react-query-devtools": "5.91.1",
		"@tanstack/react-router": "1.141.8",
		"@tanstack/react-start": "1.142.5",
		"@tanstack/react-table": "8.21.3",
		"@tanstack/react-virtual": "3.13.12",
		"chart.js": "4.5.1",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"firebase": "12.7.0",
		"firebase-admin": "13.6.0",
		"input-otp": "1.4.2",
		"lucide-react": "0.562.0",
		"next-themes": "0.4.6",
		"nitro": "3.0.1-alpha.1",
		"react": "19.2.3",
		"react-chartjs-2": "5.3.1",
		"react-dom": "19.2.3",
		"sonner": "2.0.7",
		"survey-core": "2.4.1",
		"survey-creator-core": "2.4.1",
		"survey-react-ui": "2.4.1",
		"tailwind-merge": "3.4.0",
		"tailwindcss-animate": "1.0.7",
		"tw-animate-css": "1.4.0",
		"zod": "4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@faker-js/faker": "10.1.0",
		"@happy-dom/global-registrator": "20.0.11",
		"@playwright/test": "1.57.0",
		"@tailwindcss/vite": "4.1.18",
		"@tanstack/react-router-devtools": "1.141.8",
		"@tanstack/router-plugin": "1.142.0",
		"@testing-library/dom": "10.4.1",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.0",
		"@testing-library/user-event": "14.6.1",
		"@types/node": "24.10.1",
		"@types/react": "19.2.7",
		"@types/react-dom": "19.2.3",
		"@vitejs/plugin-react": "5.1.1",
		"@vitest/coverage-v8": "4.0.16",
		"@vitest/ui": "4.0.16",
		"bun-types": "1.3.4",
		"happy-dom": "20.0.11",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"msw": "2.12.4",
		"open-cli": "8.0.0",
		"tailwindcss": "4.1.18",
		"typescript": "5.9.3",
		"vite": "7.3.0",
		"vite-plugin-pwa": "1.2.0",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "4.0.16"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"biome check --write",
			"vitest related --run --passWithNoTests"
		]
	}
}
