{
  "name": "natural-highs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "bash scripts/doppler-setup.sh",
    "dev": "doppler run -- vite",
    "build": "doppler run -- vite build",
    "preview": "vite preview",
    "server": "doppler run -- bun src/server/index.ts",
    "emulators": "doppler run -- bunx firebase-tools emulators:start",
    "dev:full": "concurrently \"bun run dev\" \"bun run server\"",
    "test": "bun run test:integration && bun run test:unit",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check:biome": "biome check .",
    "check:biome:fix": "biome check --write .",
    "commit": "commit",
    "commitlint": "commitlint --edit",
    "commitlint:last": "commitlint --last",
    "test:integration": "bunx playwright test",
    "test:unit": "bunx vitest",
    "docs:puml": "bun scripts/generate-src-plantuml.mjs"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/prompt-cli": "^20.1.0",
    "@playwright/test": "^1.28.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^5.3.11",
    "firestore-vitest-mock": "^0.6.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "chart.js": "^4.4.6",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.5.0",
    "hono": "^4.10.4",
    "html5-qrcode": "^2.3.8",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^7.9.5",
    "zod": "^4.1.12"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.1.0",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "dev",
      {
        "name": "main",
        "prerelease": false
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
