{
  "name": "natural-highs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "bun src/server/index.ts",
    "emulators": "bunx firebase-tools emulators:start",
    "dev:full": "concurrently \"bun run dev\" \"bun run server\"",
    "test": "bun run test:integration && bun run test:unit",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check:biome": "biome check .",
    "check:biome:fix": "biome check --write .",
    "test:integration": "bunx playwright test",
    "test:unit": "bunx vitest",
    "docs:puml": "bun scripts/generate-src-plantuml.mjs"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@playwright/test": "^1.28.1",
    "@types/bun": "^1.3.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "firestore-vitest-mock": "^0.6.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "chart.js": "^4.4.6",
    "dotenv": "^16.4.5",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.5.0",
    "hono": "^4.10.4",
    "html5-qrcode": "^2.3.8",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "redis": "^4.7.0"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.1.0"
}
