# Natural-Highs Environment Variables Example
# Copy to .env and fill in values
#
# Firebase Configuration (Client-side)
VITE_APIKEY=firebase-api-key
VITE_AUTH_DOMAIN=project.firebaseapp.com
VITE_PROJECT_ID=project-id
VITE_STORAGE_BUCKET=project.appspot.com
VITE_MESSAGING_SENDER_ID=000000000000
VITE_APP_ID=app-id

# Emulator Configuration
# Set to 'true' to connect to Firebase emulators
VITE_USE_EMULATORS=false

# Session Management (Server-side)
# Generate with: openssl rand -base64 32
SESSION_SECRET=session-secret-min-32-chars
